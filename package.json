{
  "name": "moltbridge",
  "version": "0.1.0",
  "description": "Professional network intelligence engine for AI agents",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "bootstrap": "tsx scripts/bootstrap-schema.ts",
    "seed": "tsx scripts/seed-graph.ts",
    "seed:sandbox": "tsx scripts/seed-sandbox.ts",
    "mcp": "tsx src/mcp/server.ts",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@noble/ed25519": "^2.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jose": "^6.0.8",
    "neo4j-driver": "^5.28.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "supertest": "^7.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
